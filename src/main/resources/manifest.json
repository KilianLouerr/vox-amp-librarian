{
  "$schema": "https://json.schemastore.org/web-manifest-combined.json",
  "name": "VOX VTX Amp Control",
  "short_name": "VTX AMP",
  "display": "standalone",
  "icons": [
    {
      "src": "icon.svg",
      "sizes": "any",
      "type": "image/svg"
    },
    {
      "src": "icon_16.png",
      "sizes": "16x16",
      "type": "image/png"
    },
    {
      "src": "icon_32.png",
      "sizes": "32x32",
      "type": "image/png"
    },
    {
      "src": "icon_64.png",
      "sizes": "64x64",
      "type": "image/png"
    },
    {
      "src": "icon_128.png",
      "sizes": "128x128",
      "type": "image/png"
    },
    {
      "src": "icon_512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "launch_handler": "focus-existing",
  "start_url": "/vox-amp-librarian",
  "theme_color": "#202020"
}